{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "crud-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/crudWebpart/CrudWebpartWebPart.js",
          "manifest": "./src/webparts/crudWebpart/CrudWebpartWebPart.manifest.json"
        }
      ]
    },
    "info-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/info/InfoWebPart.js",
          "manifest": "./src/webparts/info/InfoWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CrudWebpartWebPartStrings": "lib/webparts/crudWebpart/loc/{locale}.js",
    "InfoWebPartStrings": "lib/webparts/info/loc/{locale}.js"
  }
}
